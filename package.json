{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "android:sync:client": "npx cap sync android",
    "android:sync:admin": "cp capacitor.config.ts capacitor.config.ts.bak && cp capacitor.config.admin.ts capacitor.config.ts && npx cap sync android && mv capacitor.config.ts.bak capacitor.config.ts",
    "android:gradle:debug:client": "cd android && gradlew.bat assembleClientDebug",
    "android:gradle:debug:admin": "cd android && gradlew.bat assembleAdminDebug",
    "build:android:apk:client": "npm run build && npm run android:sync:client && npm run android:gradle:debug:client",
    "build:android:apk:admin": "npm run build && npm run android:sync:admin && npm run android:gradle:debug:admin",
    "build:android:admin": "npm run build:android:apk:admin",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:whatsapp": "gcloud run deploy whatsapp-service --source ./whatsapp-service --region us-central1 --project yasmin-beauty-diary"
  },
